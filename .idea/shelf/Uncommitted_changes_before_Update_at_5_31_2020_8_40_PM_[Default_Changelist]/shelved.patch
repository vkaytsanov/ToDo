a fa-lock\"></i>\r\n                        </div>\r\n                        <input class=\"input-scretch\" name=\"password\" type=\"password\" id=\"password\" placeholder=\"Парола\" />\r\n                        <i class=\"fas fa-eye\" id=\"show-password\"></i>\r\n                    </div>\r\n                    <div class=\"row\">\r\n                        <div id=\"repeat_password-icon\" class=\"signup-left-icon\">\r\n                            <i class=\"fa fa-lock-open\"></i>\r\n                        </div>\r\n                        <input class=\"input-scretch\" name=\"repeat_password\" id=\"repeat_password\" type=\"password\" placeholder=\"Повтори паролата\" />\r\n                    </div>\r\n                    <div class=\"row\">\r\n                        <input type=\"submit\" name=\"register\" class=\"input-scretch-more input-scretch-more-false\" value=\"Регистрирай се\" />\r\n                    </div>\r\n                </form>\r\n        </div>\r\n    </section>\r\n</main>\r\n<script type=\"text/javascript\">\r\n    $('document').ready(function() {\r\n        var input = document.getElementById(\"password\");\r\n        $('#show-password').on('click', function(){\r\n            if(input.type === \"password\"){\r\n                $('#show-password').removeClass('fa-eye');\r\n                $('#show-password').addClass('fa-eye-slash');\r\n                input.type = \"text\";\r\n\r\n            }else{\r\n                $('#show-password').removeClass('fa-eye-slash');\r\n                $('#show-password').addClass('fa-eye');\r\n                input.type = \"password\";\r\n            }\r\n        });\r\n\r\n    });\r\n</script>\r\n{% endblock %}
